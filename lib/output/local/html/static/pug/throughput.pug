extends layout.pug

block head
  style(type='text/css')
    include ../css/basscss.min.css
  style(type='text/css')
    include ../css/chartist.min.css
  style(type='text/css')
    include ../css/chartist-plugin-legend.min.css
  script(charset='utf-8' type='text/javascript')
    include ../js/chartist.min.js
  script(charset='utf-8' type='text/javascript')
    include ../js/plugins/chartist-plugin-axistitle.min.js
  script(charset='utf-8' type='text/javascript')
    include ../js/plugins/chartist-plugin-pointlabels.min.js
  script(charset='utf-8' type='text/javascript')
    include ../js/plugins/chartist-plugin-legend.min.js

block content
  div(id='min')
  script(charset='utf-8' src='./throughput-min.js' type='text/javascript')
  hr
  div(id='mean')
  script(charset='utf-8' src='./throughput-mean.js' type='text/javascript')
  hr
  div(id='max')
  script(charset='utf-8' src='./throughput-max.js' type='text/javascript')
