extends layout.pug

block head
  style(type='text/css')= style

block content
  table(class='table-light')
    thead(class='bg-darken-1')
      tr
        th Errors
        th Latency (ms)
        th Requests / Second
        th Throughput (Bytes / Second)
        th Timeouts
    tbody
      tr
        td
          a(href='./errors/index.html') Count
        td
          a(href='./latency/index.html') Mean, Min, Max &amp; Percentiles
        td
          a(href='./requests/index.html') Mean, Min &amp; Max
        td
          a(href='./throughput/index.html') Mean, Min &amp; Max
        td
          a(href='./timeouts/index.html') Count
