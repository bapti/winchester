{
  "name": "winchester",
  "version": "1.0.0",
  "description": "Soak Testing Tool",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint src",
    "test": "nyc mocha --recursive -R spec 'winchester/*.spec.js' 'winchester/**/*.spec.js'",
    "posttest": "nyc report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darArch/winchester.git"
  },
  "author": "Donald Robertson <donaldarobertson89@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darArch/winchester/issues"
  },
  "homepage": "https://github.com/darArch/winchester#readme",
  "dependencies": {
    "autocannon": "^0.15.0",
    "commander": "^2.9.0",
    "js-yaml": "^3.7.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.10.0",
    "mocha": "^3.1.2",
    "mockery": "^2.0.0",
    "nyc": "^8.4.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "nyc": {
    "check-coverage": false,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "cache": true,
    "all": true,
    "include": [
      "winchester/*.js",
      "winchester/**/*.js"
    ],
    "exclude": [
      "winchester/*.spec.js",
      "winchester/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
