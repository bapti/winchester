{
  "name": "winchester",
  "version": "1.0.0",
  "description": "Soak Testing Tool",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint src && node test/server/setUp &",
    "test": "nyc mocha --recursive -R spec 'lib/**/*.spec.js'",
    "posttest": "nyc report && node test/server/tearDown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darArch/winchester.git"
  },
  "author": "Donald Robertson <donaldarobertson89@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darArch/winchester/issues"
  },
  "homepage": "https://github.com/darArch/winchester#readme",
  "dependencies": {
    "async": "^2.1.4",
    "autocannon": "^0.15.0",
    "commander": "^2.9.0",
    "js-yaml": "^3.7.0"
  },
  "devDependencies": {
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.10.0",
    "express": "^4.14.0",
    "mocha": "^3.1.2",
    "mockery": "^2.0.0",
    "nyc": "^8.4.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "nyc": {
    "check-coverage": false,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "cache": true,
    "all": true,
    "include": [
      "lib/*.js",
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/index.js",
      "lib/**/*.min.js",
      "lib/*.spec.js",
      "lib/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
