{
  "name": "winchester",
  "version": "1.0.0",
  "description": "Soak Testing Tool",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint src",
    "test": "nyc mocha --recursive -R spec ",
    "posttest": "nyc report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darArch/winchester.git"
  },
  "author": "Donald Robertson <donaldarobertson89@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darArch/winchester/issues"
  },
  "homepage": "https://github.com/darArch/winchester#readme",
  "dependencies": {
    "autocannon": "^0.15.0",
    "commander": "^2.9.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.10.0",
    "eslint-config-google": "^0.7.0",
    "mocha": "^3.1.2",
    "nyc": "^8.4.0",
    "simple-mock": "^0.7.2"
  },
  "nyc": {
    "check-coverage": false,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "cache": true,
    "all": true,
    "include": [ "src/*.js", "src/**/*.js" ],
    "exclude": [ "test/*.js", "test/**/*.js" ],
    "reporter": [ "lcov", "text-summary" ]
  }
}
